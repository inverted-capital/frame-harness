{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc -p tsconfig.app.json --noEmit && tsc -p tsconfig.node.json --noEmit",
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "ok": "npm run format:check && npm run type-check && npm run lint && npm run build && npm test",
    "test": "npm run build",
    "preview": "vite preview",
    "up": "npx -y npm-check-updates@latest -u && npm i",
    "knip": "knip --production --fix --allow-remove-files --format --exports"
  },
  "dependencies": {
    "@artifact/client": "npm:@jsr/artifact__client@^0.0.135",
    "@privy-io/react-auth": "^2.14.0",
    "@tailwindcss/vite": "^4.1.8",
    "debug": "^4.4.1",
    "from": "^0.1.7",
    "import": "^0.0.6",
    "lucide-react": "^0.513.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.28.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "postcss": "^8.5.4",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.8",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1",
    "vite": "^6.3.5"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "trailingComma": "none"
  }
}
